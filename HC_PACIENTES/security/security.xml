<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="category_pacientes_parent" model="ir.module.category">
        <field name="name">Pacientes</field>
        <field name="sequence">2</field>
    </record>

    <record id="subcategory_pacientes" model="ir.module.category">
        <field name="name">Pacientes</field>
        <field name="sequence">1</field>
        <field name="parent_id" ref="category_pacientes_parent"/>
    </record>

    <!--groups-->
    <record id="group_user_pacientes" model="res.groups">
        <field name="name">Usuario</field>
        <field name="category_id" ref="subcategory_pacientes"/>
    </record>

    <record id="group_admin_pacientes" model="res.groups">
        <field name="name">Administrado</field>
        <field name="category_id" ref="subcategory_pacientes"/>
    </record>



</odoo>